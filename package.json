{
  "name": "obsidian-converter-root",
  "private": true,
  "type": "module",
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "npm --prefix frontend run dev",
    "dev:backend": "npm --prefix backend run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm install && npm run build",
    "build:backend": "cd backend && npm install && npm run build && cp -r dist ../dist",
    "start": "npm --prefix backend run start",
    "test": "npm --prefix backend run test",
    "lint": "npm --prefix backend run lint",
    "install:all": "npm install"
  },
  "dependencies": {
    "@bundled-es-modules/pdfjs-dist": "^3.6.172-alpha.1",
    "@ffprobe-installer/ffprobe": "^2.1.2",
    "@iarna/rtf-to-html": "^1.1.0",
    "@smui/button": "7.0.0",
    "@smui/linear-progress": "7.0.0",
    "@smui/textfield": "7.0.0",
    "@squoosh/lib": "^0.3.1",
    "archiver": "^7.0.1",
    "file-saver": "^2.0.5",
    "file-type": "^19.6.0",
    "jszip": "^3.10.1",
    "mammoth": "^1.4.21",
    "node-fetch": "^3.3.2",
    "p-limit": "^3.1.0",
    "sanitize-filename": "^1.6.3",
    "svelte-spinner": "^2.0.2",
    "turndown": "^7.2.0",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@smui/common": "^7.0.0",
    "@sveltejs/adapter-auto": "^3.2.5",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "axios": "^1.7.7",
    "canvas": "^2.11.2",
    "cheerio": "^1.0.0",
    "concurrently": "^8.0.0",
    "eslint": "^7.32.0",
    "husky": "^8.0.0",
    "jsdom": "^25.0.1",
    "lint-staged": "^13.1.0",
    "nodemon": "^2.0.12",
    "office-text-extractor": "^3.0.3",
    "officeparser": "^5.0.0",
    "path-browserify": "^1.0.1",
    "puppeteer": "^23.6.0",
    "sass": "^1.79.4",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tmp-promise": "^3.0.3",
    "vite": "^5.0.3",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "rollup": "^3.20.0",
    "@sveltejs/adapter-node": "^2.0.0"
  },
  "overrides": {
    "pdf-export-images": {
      "pdfjs-dist": "3.11.174"
    }
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
